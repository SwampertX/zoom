{

  "manifest_version": 2,
  "name": "zoom",
  "version": "1.0",

  "description": "Zooms to default level on any page.",

  "icons": {
    
  },

  "permissions": ["activeTab", "storage"],

  "options_ui": {
    "page": "popup/settings.html"
  },

  "browser_action": {
    "default_icon": "",
    "default_title": "zoom",
    "default_popup": "popup/settings.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["zoom.js"]
    }
  ]

}