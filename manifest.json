{

  "manifest_version": 2,
  "name": "zoom",
  "version": "1.0",

  "description": "Zooms to default level on any page.",

  "icons": {
    
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "",
    "default_title": "zoom",
    "default_popup": "popup/settings.html"
  },

  "content_scripts": [
    {
      "matches": ["*"],
      "js": ["zoom.js"]
    }
  ]

}